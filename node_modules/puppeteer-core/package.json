    "build": "wireit",
    "build:docs": "wireit",
    "build:third_party": "wireit",
    "build:tsc": "wireit",
    "build:types": "wireit",
    "check": "tsx tools/ensure-correct-devtools-protocol-package",
    "clean": "tsc -b --clean && rimraf lib src/generated",
    "format:types": "wireit",
    "generate:package-json": "wireit",
